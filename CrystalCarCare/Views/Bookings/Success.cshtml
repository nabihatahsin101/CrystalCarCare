@model CrystalCarCare.Models.Booking

@{
    ViewBag.Title = "Booking Confirmed";
}

<h2>Booking Confirmed!</h2>

<p>Your booking ID: <b>@Model.BookingId</b></p>
<p>Service: @Model.ServiceName</p>
<p>Price: $@Model.Price</p>
